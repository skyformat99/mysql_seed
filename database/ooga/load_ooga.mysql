# database/ooga/load_ooga.mysql
#
# seeds MySQL database ooga according to the tabular data files:
#
# 	- database/ooga/booga.csv

CREATE DATABASE ooga;

USE ooga;

CREATE TABLE booga (
	looga INT
);

LOAD DATA INFILE 'database/ooga/booga.csv'
	IGNORE INTO TABLE booga
	FIELDS TERMINATED BY ','
	       ENCLOSED BY   ''
	       ESCAPED BY    '\\'
	LINES  STARTING BY   ''
	       TERMINATED BY '\n'
	IGNORE 17 LINES;