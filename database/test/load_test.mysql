# database/test/load_test.mysql
#
# seeds MySQL database test according to the tabular data files:
#
# 	- database/test/test.tsv

USE test;

LOAD DATA INFILE 'database/test/test.tsv'
	IGNORE INTO TABLE test
	FIELDS TERMINATED BY '\t'
	       ENCLOSED BY   ''
	       ESCAPED BY    '\\'
	LINES  STARTING BY   ''
	       TERMINATED BY '\\'
	IGNORE 18 LINES;