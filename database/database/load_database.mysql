# database/database/load_database.mysql
#
# seeds MySQL database database according to the tabular data files:
#
# 	- database/database/table.tsv

USE database;

LOAD DATA INFILE 'database/database/table.tsv'
	IGNORE INTO TABLE table
	FIELDS TERMINATED BY '\t'
	       ENCLOSED BY   ''
	       ESCAPED BY    '\\'
	LINES  STARTING BY   ''
	       TERMINATED BY '\\'
	IGNORE 18 LINES;